import streamlit as st
import pandas as pd
import numpy as np

# Tạo dữ liệu mẫu
dates = pd.date_range(start="2023-01-01", periods=100)
data = np.random.randn(100, 1)

# Tạo dataframe
df = pd.DataFrame(data, columns=['Giá trị'], index=dates)

# Vẽ biểu đồ đường
st.title("Biểu đồ đường (Line Chart)")
st.line_chart(df)
streamlit run your_script.py
